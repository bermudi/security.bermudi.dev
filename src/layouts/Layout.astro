---
import BaseLayout from './BaseLayout.astro';
import Navbar from '../components/Navbar';
import Footer from '../components/Footer';  // Assuming you have a Footer component

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<BaseLayout title={title}>
  <Navbar client:load />
  <main>
    <slot />
  </main>
</BaseLayout>